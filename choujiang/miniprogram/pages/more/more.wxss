/* miniprogram/pages/more/more.wxss */
@import '../../style/common.wxss';

.page {
  width: 100%;
  height: 100%;
  padding-top: 10px;
}
.container {
  width: 100%;
  background-color:#fff;
  padding:0 10px;
}
.item {
  padding: 15px 0px;
  font-size: 16px;
  width: 100%;
  display: flex;
}
.left {
  flex: 1;
}
.item image {
  width: 15px;
  height: 15px;
}
.text {
  margin-left: 5px;
}
.complain {
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
}