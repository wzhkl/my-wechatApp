/* miniprogram/pages/allDraw/allDraw.wxss */
@import "../../style/common.wxss";

.page {
  width: 100%;
  height: 100%;
  padding-top: 40rpx;
}
.waited, .over {
  width:100%;
  margin-bottom: 40rpx;
}
.waited-header, .over-title {
  width: 100%;
  font-size: 14px;
  opacity: 0.7;
  padding-left: 20rpx;
  margin-bottom: 20rpx;
}
.prize {
  /* margin-left: 10rpx; */
  font-size: 16px;
  width: 450rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.prizeDay {
  font-size: 14px;
  opacity: 0.7;
}
.over-prize {
  background-color: #fff;
  padding: 40rpx 20rpx;
  display:flex;
  justify-content:space-between;
  align-items:center;
}
.over-prize image {
  width: 30rpx;
  height: 30rpx;
}
