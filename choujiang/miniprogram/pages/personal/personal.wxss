/* miniprogram/pages/personal/personal.wxss */
@import "../../style/common.wxss";
.page {
  width: 100%;
  height: 100%;
}
.container {
  padding: 0 10px;
}
.header {
  padding: 20px 0px;
  border-bottom: 1px solid #dad5d5;
  width: 100%;
}
.userName {
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 10px;
}
.count {
  font-size: 14px;
  opacity: 0.7;
}
.body {
  margin-top: 10px;
  padding: 30px 20px;
  text-align: center;
  background-color:#ffffff;
}
.icon image {
  width: 50px;
  height: 50px;
}
.title {
  font-size: 16px;
  font-weight: bold;
  margin: 30px 0;
}
.details, .process {
  font-size: 14px;
  opacity: 0.8;
}
.process {
  margin: 30px 0;
}
.identify {
  text-align: center;
  padding: 10px 0px;
  background-color: #575757;
  color:#ffffff;
  margin:60px 0px;
}


